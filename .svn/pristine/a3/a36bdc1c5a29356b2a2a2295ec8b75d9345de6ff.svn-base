		</dependency>

		<dependency>
		  <groupId>xpp3</groupId>
		  <artifactId>xpp3_min</artifactId>
		  <version>1.1.4c</version>
		</dependency>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>jstl</artifactId>
		</dependency>
		<dependency>
			<groupId>taglibs</groupId>
			<artifactId>standard</artifactId>
		</dependency>
		
